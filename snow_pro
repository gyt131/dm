-- State Table
CREATE TABLE Dim_State (
    StateID INT PRIMARY KEY,
    StateName VARCHAR(50)
);

-- City Table
CREATE TABLE Dim_City (
    CityID INT PRIMARY KEY,
    CityName VARCHAR(50),
    StateID INT,
    FOREIGN KEY (StateID) REFERENCES Dim_State(StateID)
);


CREATE TABLE Dim_Customer_SF (
    CustomerID INT PRIMARY KEY,
    CustomerName VARCHAR(50),
    CityID INT,
    FOREIGN KEY (CityID) REFERENCES Dim_City(CityID)
);


CREATE TABLE Dim_Product_SF (
    ProductID INT PRIMARY KEY,
    ProductName VARCHAR(50),
    Category VARCHAR(50)
);


CREATE TABLE Dim_Store_SF (
    StoreID INT PRIMARY KEY,
    StoreName VARCHAR(50),
    CityID INT,
    FOREIGN KEY (CityID) REFERENCES Dim_City(CityID)
);


CREATE TABLE Dim_Time_SF (
    TimeID INT PRIMARY KEY,
    Day INT,
    Month INT,
    Year INT
);


CREATE TABLE Fact_Sales_SF (
    SalesID INT PRIMARY KEY,
    CustomerID INT,
    ProductID INT,
    StoreID INT,
    TimeID INT,
    Quantity INT,
    Amount DECIMAL(10,2),
    FOREIGN KEY (CustomerID) REFERENCES Dim_Customer_SF(CustomerID),
    FOREIGN KEY (ProductID) REFERENCES Dim_Product_SF(ProductID),
    FOREIGN KEY (StoreID) REFERENCES Dim_Store_SF(StoreID),
    FOREIGN KEY (TimeID) REFERENCES Dim_Time_SF(TimeID)
);
